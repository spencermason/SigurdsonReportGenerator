<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="652.0" styleClass="background" stylesheets="@../styles/styles.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sigurdsonreportgenerator.MainFormController">
   <children>
      <Button alignment="CENTER" layoutX="10.0" layoutY="235.0" mnemonicParsing="false" onAction="#addReportType" prefHeight="100.0" prefWidth="210.0" styleClass="button1" text="New">
         <font>
            <Font name="Calibri" size="27.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button alignment="CENTER" layoutX="221.0" layoutY="235.0" mnemonicParsing="false" onAction="#editReport" prefHeight="100.0" prefWidth="210.0" styleClass="button2" text="Edit">
         <font>
            <Font name="Arial" size="27.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button alignment="CENTER" layoutX="432.0" layoutY="235.0" mnemonicParsing="false" onAction="#generateReport" prefHeight="100.0" prefWidth="210.0" styleClass="button3" text="Generate">
         <font>
            <Font size="27.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="AddEmployee" layoutX="200.0" layoutY="346.0" mnemonicParsing="false" onAction="#addEmployee" prefHeight="40.0" prefWidth="100.0" styleClass="buttonGeneral" text="Add Employee" />
      <Button fx:id="EditEmployee" layoutX="353.0" layoutY="346.0" mnemonicParsing="false" onAction="#editEmployee" prefHeight="40.0" prefWidth="100.0" styleClass="buttonGeneral" text="Edit Employee" />
   </children>
</AnchorPane>
